{
  "name": "@samples/atomic-search-commerce-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm run build:assets && npm run dev:vite",
    "build:assets": "node ../../../utils/ci/copy-files.mjs ../../../packages/atomic-react/dist/assets public/assets ../../../packages/atomic-react/dist/lang public/lang ../../../packages/atomic/dist/atomic/themes public/themes",
    "dev:vite": "vite",
    "build": "npm run build:ts && npm run build:vite",
    "build:ts": "npm run-script -w=@coveo/ci if-not-cdn -- \"tsc -b\"",
    "build:vite": "npm run-script -w=@coveo/ci if-not-cdn -- \"vite build\""
  },
  "dependencies": {
    "@coveo/atomic-react": "3.3.18",
    "@coveo/headless": "3.32.0",
    "react": "19.1.1",
    "react-dom": "19.1.1"
  },
  "devDependencies": {
    "@playwright/test": "1.56.0",
    "@types/node": "22.18.9",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@vitejs/plugin-react": "5.0.3",
    "typescript": "5.9.3",
    "vite": "7.1.9"
  }
}
