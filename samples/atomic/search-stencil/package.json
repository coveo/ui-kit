{
  "name": "@samples/atomic-search-stencil",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "stencil build --dev --watch --serve --port 3666 --no-open",
    "build": "pnpm --filter @coveo/ci if-not-cdn -- \"pnpm exec node --max_old_space_size=6144 ./node_modules/@stencil/core/bin/stencil build\"",
    "e2e:watch": "cypress open --browser chrome --e2e"
  },
  "dependencies": {
    "@coveo/atomic": "workspace:*",
    "@coveo/headless": "workspace:*",
    "@stencil/core": "catalog:",
    "stencil-router-v2": "0.6.0"
  },
  "devDependencies": {
    "cypress": "catalog:",
    "cypress-repeat": "catalog:",
    "rollup-plugin-string": "catalog:",
    "rollup-plugin-node-polyfills": "catalog:"
  }
}
