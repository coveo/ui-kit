{
  "enabled": true,
  "extends": [":semanticPrefixFixDepsChoreOthers"],
  "schedule": ["before 2am"],
  "packageRules": [
    {
      "matchPackagePatterns": ["*"],
      "groupName": "all dependencies",
      "groupSlug": "all"
    }
  ],
  "rangeStrategy": "auto",
  "lockFileMaintenance": {
    "enabled": false
  },
