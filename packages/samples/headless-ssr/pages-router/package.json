{
  "name": "@coveo/headless-ssr-samples-pages-router",
  "description": "Examples of framework agnostic @coveo/headless/ssr utils and @coveo/headless-react/ssr using Next.js pages router",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "nx build",
    "build:next": "next build",
    "prod": "next start",
    "lint": "next lint",
    "e2e": "cypress run --browser chrome --e2e --project ../ --env NEXTJS_ROUTER=pages",
    "e2e:watch": "cypress open --browser chrome --e2e --project ../"
  },
  "dependencies": {
    "@coveo/headless-ssr-samples-common": "0.0.0",
    "next": "14.2.20",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "22.10.2",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "eslint": "8.57.1",
    "typescript": "5.5.4"
  }
}
