{
  "name": "@coveo/headless-ssr-samples-app-router",
  "description": "Examples of framework agnostic @coveo/headless/ssr utils and @coveo/headless-react/ssr using Next.js app router",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "nx build",
    "build:next": "next build",
    "prod": "next start",
    "lint": "next lint",
    "e2e": "cypress run --browser chrome --e2e --project ../ --env NEXTJS_ROUTER=app",
    "e2e:watch": "cypress open --browser chrome --e2e --project ../"
  },
  "dependencies": {
    "@coveo/headless-ssr-samples-common": "0.0.0",
    "next": "14.0.4",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/node": "20.10.7",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "eslint": "8.56.0",
    "eslint-config-next": "14.0.4",
    "typescript": "5.3.3"
  }
}
