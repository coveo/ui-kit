{
  "name": "@coveo/atomic-react-samples",
  "version": "0.0.0",
  "description": "Samples with atomic-react",
  "private": true,
  "dependencies": {
    "@coveo/atomic": "2.31.3",
    "@coveo/atomic-react": "2.2.37",
    "@coveo/headless": "2.18.3",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "css-loader": "6.7.1",
    "cypress": "10.8.0",
    "eslint-plugin-react": "7.30.1",
    "gts": "^3.1.1",
    "ncp": "2.0.0",
    "style-loader": "3.3.1",
    "ts-loader": "9.3.1",
    "typescript": "4.6.4",
    "webpack": "^5.83.1",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.10.0"
  },
  "scripts": {
    "build": "tsc --noEmit && tsc --module commonjs --noEmit",
    "dev": "npm run build:assets && npm run dev:webpack",
    "dev:webpack": "webpack serve",
    "build:assets": "ncp ../../atomic-react/dist/assets public/assets && ncp ../../atomic-react/dist/lang public/lang public/assets && ncp ../../atomic/dist/atomic/themes public/themes",
    "e2e:watch": "cypress open --browser chrome --e2e"
  }
}
