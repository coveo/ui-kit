{
  "name": "angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run link:local && npm run copy:assets && ng serve",
    "watch": "ng build --watch --configuration development",
    "link:local": "npm link ../../atomic-angular/projects/atomic-angular/dist/",
    "copy:assets": "ncp node_modules/@coveo/atomic-angular/assets src/assets && ncp node_modules/@coveo/atomic-angular/lang src/lang"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.1.0",
    "@angular/common": "~13.1.0",
    "@angular/compiler": "~13.1.0",
    "@angular/core": "~13.1.0",
    "@angular/forms": "~13.1.0",
    "@angular/platform-browser": "~13.1.0",
    "@angular/platform-browser-dynamic": "~13.1.0",
    "@angular/router": "~13.1.0",
    "@coveo/atomic": "^1.23.7",
    "@coveo/atomic-angular": "^0.0.1",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.1.4",
    "@angular/cli": "~13.1.4",
    "@angular/compiler-cli": "~13.1.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "ncp": "^2.0.0",
    "typescript": "~4.5.2"
  }
}
