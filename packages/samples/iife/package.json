{
  "name": "@coveo/iife-samples",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "rimraf www/cdn",
    "dev": "npm run build && npm run prod",
    "prod": "serve www",
    "build": "npm run-script -w=@coveo/ci if-not-cdn -- \"npm run build:assets\"",
    "build:assets": "node build-assets.mjs",
    "e2e:watch": "cypress open --browser chrome --e2e",
    "e2e": "cypress run --browser chrome"
  },
  "dependencies": {
    "@babel/standalone": "7.28.1",
    "@coveo/atomic": "3.33.1",
    "@coveo/atomic-hosted-page": "1.2.5",
    "@coveo/atomic-react": "3.3.5",
    "@coveo/headless": "3.29.0",
    "react": "19.1.1",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "cypress": "13.7.3",
    "resolve": "1.22.10",
    "rimraf": "6.0.1",
    "serve": "14.2.4"
  }
}
