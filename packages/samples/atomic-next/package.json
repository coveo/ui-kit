{
  "name": "@coveo/atomic-next-samples",
  "version": "0.0.0",
  "private": true,
  "dependencies": {
    "@coveo/atomic": "2.37.0",
    "@coveo/atomic-react": "2.5.1",
    "@coveo/headless": "2.23.0",
    "next": "13.2.3",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/node": "17.0.34",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "cypress": "10.8.0",
    "ncp": "2.0.0",
    "typescript": "4.6.4"
  },
  "scripts": {
    "build": "tsc --noEmit && tsc --module commonjs --noEmit",
    "dev": "npm run build:assets && npm run dev:nextjs",
    "dev:nextjs": "next dev",
    "build:assets": "ncp ../../atomic-react/dist/assets public/assets && ncp ../../atomic-react/dist/lang public/lang public/assets",
    "e2e:watch": "cypress open --browser chrome --e2e",
    "e2e": "cypress run --browser chrome"
  }
}
