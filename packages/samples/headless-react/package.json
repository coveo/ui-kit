{
  "name": "headless-react",
  "version": "0.50.21",
  "dependencies": {
    "@coveo/atomic-react": "^0.5.4",
    "@coveo/auth": "^1.1.8",
    "@coveo/headless": "^1.47.0",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "11.2.7",
    "@testing-library/user-event": "13.5.0",
    "@types/escape-html": "1.0.1",
    "@types/express": "4.17.13",
    "@types/jest": "26.0.24",
    "@types/node": "12.20.37",
    "@types/react": "16.14.21",
    "@types/react-dom": "17.0.11",
    "@types/react-router-dom": "5.3.2",
    "dayjs": "1.10.7",
    "escape-html": "1.0.3",
    "express": "4.17.1",
    "filesize": "6.4.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "5.3.0",
    "react-scripts": "4.0.3",
    "typescript": "4.5.2",
    "web-vitals": "1.1.2"
  },
  "scripts": {
    "prepublishOnly": "exit 1",
    "dev": "npm run copy:assets && npm run link:atomic-react && react-scripts start",
    "build": "tsc --noEmit",
    "build:server": "tsc --project ./tsconfig.server.json",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "ssr:build": "npm run build && npm run build:server",
    "ssr:start": "node ./build/server/server/server.js",
    "copy:assets": "ncp node_modules/@coveo/atomic-react/dist/assets public/assets && ncp node_modules/@coveo/atomic-react/dist/lang public/lang",
    "link:atomic-react": "cd ../../atomic-react && npm link ../samples/headless-react/node_modules/react/",
    "cypress:open": "cypress open"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cypress": "^9.3.1",
    "eslint-plugin-cypress": "^2.12.1",
    "ncp": "^2.0.0",
    "patch-package": "^6.4.7"
  }
}
