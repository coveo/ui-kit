{
  "name": "headless-react",
  "description": "Samples with @coveo/headless & React",
  "version": "0.0.0",
  "private": true,
  "dependencies": {
    "@coveo/auth": "^1.7.6",
    "@coveo/headless": "^1.77.6",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.3.0",
    "@testing-library/user-event": "14.2.1",
    "@types/escape-html": "1.0.2",
    "@types/express": "4.17.13",
    "@types/node": "18.0.0",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "@types/react-router-dom": "5.3.3",
    "jest-watch-typeahead": "1.1.0",
    "dayjs": "1.11.2",
    "escape-html": "1.0.3",
    "express": "4.18.1",
    "filesize": "6.4.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "5.3.1",
    "react-scripts": "5.0.1",
    "typescript": "4.6.4",
    "web-vitals": "1.1.2"
  },
  "scripts": {
    "dev": "react-scripts start",
    "build": "tsc --noEmit && tsc --module commonjs --noEmit",
    "build:server": "tsc --project ./tsconfig.server.json",
    "test": "react-scripts test --env=./jest.setup.js",
    "eject": "react-scripts eject",
    "ssr:build": "npm run build && npm run build:server",
    "ssr:start": "node ./build/server/server/server.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "patch-package": "6.4.7"
  }
}
