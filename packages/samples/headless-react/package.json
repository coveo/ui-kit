{
  "name": "headless-react",
  "private": "true",
  "version": "0.60.0",
  "dependencies": {
    "@coveo/auth": "1.7.0",
    "@coveo/headless": "1.77.0",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "11.2.7",
    "@testing-library/user-event": "13.5.0",
    "@types/escape-html": "1.0.2",
    "@types/express": "4.17.13",
    "@types/jest": "26.0.24",
    "@types/node": "12.20.52",
    "@types/react": "16.14.26",
    "@types/react-dom": "17.0.17",
    "@types/react-router-dom": "5.3.3",
    "dayjs": "1.11.2",
    "escape-html": "1.0.3",
    "express": "4.18.1",
    "filesize": "6.4.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "5.3.1",
    "react-scripts": "4.0.3",
    "typescript": "4.6.4",
    "web-vitals": "1.1.2"
  },
  "scripts": {
    "prepublishOnly": "exit 1",
    "dev": "npm run copy:assets && npm run link:atomic-react && react-scripts start",
    "build": "tsc --noEmit && tsc --module commonjs --noEmit",
    "build:server": "tsc --project ./tsconfig.server.json",
    "test": "react-scripts test --env=./jest.setup.js",
    "eject": "react-scripts eject",
    "ssr:build": "npm run build && npm run build:server",
    "ssr:start": "node ./build/server/server/server.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "patch-package": "6.4.7"
  }
}
