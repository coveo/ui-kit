  "dependencies": {
    "next": "15.2.4",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "@coveo/atomic-react": "3.3.6",
    "@coveo/atomic": "3.33.2",
    "@coveo/headless": "3.29.1"
  },
  "devDependencies": {
    "typescript": "5.9.2",
    "@types/node": "22.17.2",
    "@types/react": "19.1.1",
    "@types/react-dom": "19.1.2",
    "ncp": "2.0.0"
  }
}
