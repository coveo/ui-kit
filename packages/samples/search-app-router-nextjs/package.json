    "next": "15.2.4",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "@coveo/atomic-react": "3.3.2",
    "@coveo/atomic": "3.32.2",
    "@coveo/headless": "3.28.2"
  },
  "devDependencies": {
    "typescript": "5.8.3",
    "@types/node": "22.16.5",
    "@types/react": "19.1.1",
    "@types/react-dom": "19.1.2",
    "ncp": "2.0.0"
  }
}
