{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "../node_modules",
    "target": "es6",
    "lib": ["esnext", "dom"],
    "types": ["node", "cypress", "cypress-real-events"],
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true
  },
  "include": ["../node_modules/cypress", "**/*.ts", "**/*.js"]
}
