        {
            "name": "QuanticFacet",
            "description": "<p>A facet is a list of values for a certain field occurring in the results, ordered using a configurable criteria (e.g., number of occurrences).<br>\nA <code>QuanticFacet</code> displays a facet of the results for the current query.</p>",
            "access": "",
            "fires": "",
            "examples": [
                "<c-quantic-facet field=\"filetype\" label=\"File Type\" engine-id={engineId}></c-quantic-facet>"
            ],
            "category": "LWC",
            "properties": [
                {
                    "name": "facetId",
                    "access": "@api",
                    "description": "<p>An unique ID used to identify the facet instance.\nDefaults to given facet label.</p>",
                    "required": false,
                    "type": {
                        "name": "string",
                        "defaultValue": "(label)"
                    }
                },
                {
                    "name": "field",
                    "access": "@api",
                    "description": "<p>The field whose values you want to display in the facet.</p>",
                    "required": true,
                    "type": {
                        "name": "string",
                        "defaultValue": ""
                    }
                },
                {
                    "name": "label",
                    "access": "@api",
                    "description": "<p>The non-localized label for the facet.</p>",
                    "required": true,
                    "type": {
                        "name": "string",
                        "defaultValue": ""
                    }
                },
                {
                    "name": "engineId",
                    "access": "@api",
                    "description": "<p>The ID of the engine instance with which to register.</p>",
                    "required": true,
                    "type": {
                        "name": "string",
                        "defaultValue": ""
                    }
                },
                {
                    "name": "numberOfValues",
                    "access": "@api",
                    "description": "<p>The number of values to request for this facet.\nAlso determines the number of additional values to request each time this facet is expanded, and the number of values to display when this facet is collapsed.</p>",
                    "required": false,
                    "type": {
                        "name": "number",
                        "defaultValue": "8"
                    }
                },
                {
                    "name": "sortCriteria",
                    "access": "@api",
                    "description": "<p>The sort criterion to apply to the returned facet values\nPossible values are:</p>\n<ul>\n<li>score</li>\n<li>numeric</li>\n<li>occurences</li>\n<li>automatic</li>\n</ul>",
                    "required": false,
                    "type": {
                        "name": "FacetSortCriterion",
                        "defaultValue": "automatic"
                    }
                },
                {
                    "name": "noSearch",
                    "access": "@api",
                    "description": "<p>Whether this facet should not contain a search box.</p>",
                    "required": false,
                    "type": {
                        "name": "boolean",
                        "defaultValue": "false"
                    }
                }
            ]
        },
