                    "required": false,
                    "type": {
                        "name": "RangeFacetSortCriterion",
                        "defaultValue": "'ascending'"
                    }
                },
                {
                    "name": "rangeAlgorithm",
                    "access": "@api",
                    "description": "<p>The algorithm that’s used for generating the ranges of this facet when they aren’t manually defined.\nThe default value of &quot;even&quot; generates equally sized facet ranges across all of the results.\nThe value &quot;equiprobable&quot; generates facet ranges which vary in size but have a more balanced number of results within each range.</p>",
                    "required": false,
                    "type": {
                        "name": "RangeFacetRangeAlgorithm",
                        "defaultValue": "'equiprobable'"
                    }
                },
