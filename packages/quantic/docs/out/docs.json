{
    "classes": [
        {
            "name": "QuanticDateFacet",
            "description": "<p>The <code>QuanticDateFacet</code> component displays facet values as date ranges. In mobile browsers, this is rendered as a button that opens a facet modal.</p>",
            "access": "",
            "fires": "",
            "examples": [
                "<c-quantic-date-facet field=\"date\" label=\"Date\" engine-id={engineId}></c-quantic-date-facet>"
            ],
            "category": "LWC",
            "properties": [
                {
                    "name": "facetId",
                    "access": "@api",
                    "description": "<p>Specifies a unique identifier for the facet.</p>",
                    "required": false,
                    "type": {
                        "name": "string",
                        "defaultValue": "[field]"
                    }
                },
                {
                    "name": "field",
                    "access": "@api",
                    "description": "<p>Specifies the index field whose values the facet should use.</p>",
                    "required": true,
                    "type": {
                        "name": "string",
                        "defaultValue": ""
                    }
                },
                {
                    "name": "label",
                    "access": "@api",
                    "description": "<p>The non-localized label for the facet.</p>",
                    "required": true,
                    "type": {
                        "name": "string",
                        "defaultValue": ""
                    }
                },
                {
                    "name": "engineId",
                    "access": "@api",
                    "description": "<p>The ID of the engine instance with which to register.</p>",
                    "required": true,
                    "type": {
                        "name": "string",
                        "defaultValue": ""
                    }
                },
                {
                    "name": "numberOfValues",
                    "access": "@api",
                    "description": "<p>The number of values to request for this facet, when there are no manual ranges.</p>",
                    "required": false,
                    "type": {
                        "name": "number",
                        "defaultValue": "8"
                    }
                },
                {
                    "name": "formattingFunction",
                    "access": "@api",
                    "description": "<p>The function used to format the date facet value label.</p>",
                    "required": false,
                    "type": {
                        "name": "function",
                        "defaultValue": "Formatted result: [start] - [end]",
                        "params": [
                            {
                                "type": {
                                    "names": [
                                        "DateFacetValue"
                                    ]
                                }
                            }
                        ],
                        "returns": [
                            {
                                "type": {
                                    "names": [
                                        "string"
                                    ]
                                }
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
