
{
  "tags": {
    "allowUnknownTags": true
  },
  "plugins": [
    "plugins/markdown",
    "./node_modules/better-docs/category",
    "./node_modules/better-docs/typescript",
    "./node_modules/better-docs/typedef-import",
    "./node_modules/@ckeditor/jsdoc-plugins/lib/export-fixer/export-fixer.js"
  ],
  "source": {
    "include": ["./force-app/main/default/lwc/quanticFacet"],
    "includePattern": "quantic.*.js$",
    "excludePattern": "(node_modules/|example.*.js$)"
  },
  "opts": {
    "destination": "console",
    "encoding": "utf8",
    "recurse": true,
    "template": "templates/haruki"
  }
}