@import '../../../global/global.pcss';
@import '../../../utils/tailwind-utilities/aspect-square.tw.css';

@layer utilities {
  atomic-icon {
    @apply inline-block;
    fill: currentColor;
    @apply aspect-square-[auto];
    > svg {
      width: 100%;
      max-height: 100%;
      @apply aspect-square-[auto];
    }
  }
}
