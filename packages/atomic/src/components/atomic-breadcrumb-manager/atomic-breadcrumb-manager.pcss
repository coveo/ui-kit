@import "tailwindcss/base";
@import "tailwindcss/utilities";
@import "../../global/global.pcss";

/**
 * @prop --max-breadcrumb-width: Maximum width of a breadcrumb before the text is truncated
 */
.breadcrumb {
  max-width: var(--max-breadcrumb-width, 155px);
}

.breadcrumb-clear svg {
  @apply w-3 h-3;
}
