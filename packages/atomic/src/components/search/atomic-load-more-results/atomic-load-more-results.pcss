@import '../../../global/global.pcss';

/**
 * @prop --atomic-more-results-progress-bar-color-from: Color of the start of the gradient for the load more results progress bar.
 * @prop --atomic-more-results-progress-bar-color-to: Color of the end of the gradient for the load more results progress bar.
 */
.progress-bar {
  @apply from-more-results-progress-bar-color-from;
  @apply to-more-results-progress-bar-color-to;
}
