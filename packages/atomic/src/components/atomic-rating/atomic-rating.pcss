/**
 * @prop --atomic-rating-facet-icon-active-color: Color of the icon when active.
 */
.icon-active {
  color: var(--atomic-rating-facet-icon-active-color, #f4c315);
}

/**
 * @prop --atomic-rating-facet-icon-inactive-color: Color of the icon when inactive.
 */
.icon-inactive {
  color: var(--atomic-rating-facet-icon-inactive-color, var(--atomic-neutral));
}
